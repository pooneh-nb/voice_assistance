{
    "name": "MyFirefoxProxy",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "browsingData",
        "proxy",
        "storage",
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "downloads",
        "notifications",
        "<all_urls>"
    ],
    "icons": {
        "48": "icons/icon.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}